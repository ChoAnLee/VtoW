{
    "manifest_version": 3,
    "name": "Multilingual IME",
    "version": "1.0",
    "description": "Multilingual IME",
    "permissions": ["activeTab", "input"],
    "host_permissions": [
        "https://*/*"
    ],
    "action": {
        "default_popup": "popup.html",

        "default_icon": {
            "48": "images/try.png"
        }
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "48": "images/try.png"
    },
    "web_accessible_resources": [{
        "resources": ["src/bopomofo_dict_with_frequency2.json", "src/canjie_dict_with_frequency.json"],
        "matches": ["*://*/*"],
        "extension_ids": ["*"]
    }]
}